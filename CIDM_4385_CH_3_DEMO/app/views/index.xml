<Alloy>
	<Window class="container">
		<Label id="label" onClick="doClick">Hello, World</Label>
		<Collection src="cars">
			<Window id-"mainWindow">
				<TableView dataCollection="cars" dataTransform="transform" dataFilter="filter">
					<TableViewRow title="{Title}" modelId="{id}" /
				</TableView>
			</Window>
		</Collection>
	</Window>
</Alloy>